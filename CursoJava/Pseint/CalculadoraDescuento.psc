Algoritmo CalculadoraDescuento
	
	Definir precio,descuento,final Como Real;
	
	Escribir "Ingrese el precio del articulo";
	leer precio;
	
	Escribir "Ingrese el descuento a realizar";
	leer descuento;
	
	final= (precio*descuento)/100;
	
	Escribir "El precio final despues de aplicar el descuento es: $" , final;
	
FinAlgoritmo
